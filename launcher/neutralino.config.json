{
	"applicationId": "dev.souped.launcher",
	"version": "1.0.0",
	"defaultMode": "window",
	"port": 0,
	"documentRoot": "/resources/",
	"url": "/",
	"enableServer": true,
	"enableNativeAPI": true,
	"tokenSecurity": "one-time",
	"logging": {
		"enabled": true,
		"writeToLogFile": true
	},
	"nativeAllowList": [
		"os.*",
		"filesystem.*"
	],
	"globalVariables": {},
	"modes": {
		"window": {
			"title": "launcher",
			"width": 500,
			"height": 500,
			"minWidth": 400,
			"minHeight": 400,
			"fullScreen": false,
			"alwaysOnTop": true,
			"icon": "resources/icons/logo.svg",
			"enableInspector": false,
			"borderless": false,
			"maximize": false,
			"hidden": false,
			"resizable": true,
			"exitProcessOnClose": true
		}
	},
	"cli": {
		"binaryName": "launcher",
		"resourcesPath": "/resources/",
		"extensionsPath": "/extensions/",
		"clientLibrary": "/resources/js/neutralino.js",
		"binaryVersion": "4.5.0",
		"clientVersion": "3.4.0"
	}
}
